{
    "name": "xu42/mydlpu",
    "description": "The DLPU ",
    "keywords": ["dlpu"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "overtrue/laravel-wechat": "~3.0",
        "vlucas/phpdotenv": "~2.2",
        "monolog/monolog": "^1.22",
        "xu42/cet": "^3.0",
        "xu42/dlpu-news": "^3.0",
        "xu42/express-tracking": "^3.0",
        "xu42/qznjw2014": "^3.0",
        "xu42/dlpu-network": "^3.0",
        "xu42/dlpu-ecard": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
